{
	"name": "craft",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/themes/craft/src",
	"prefix": "lib",
	"projectType": "library",
	"tags": [],
	"targets": {
		"build-scss": {
			"executor": "nx:run-commands",
			"outputs": ["{options.outputPath}"],
			"options": {
				"command": "sass libs/themes/craft/src/sass:dist/libs/themes/craft",
				"outputPath": "libs/themes/craft"
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/themes/craft/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint"
		}
	}
}
